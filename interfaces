allow-hotplug wlan0
iface wlan0 inet static
        address 192.168.1.111
        network 192.168.1.0
        broadcast 192.168.1.255
        netmask 255.255.255.0
wpa-conf /etc/wpa_supplicant/wpa_supplicant.conf








